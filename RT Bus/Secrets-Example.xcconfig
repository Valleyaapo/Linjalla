// Your Digitransit API Key
DIGITRANSIT_API_KEY = YOUR_DIGITRANSIT_API_KEY_HERE

// HSL MQTT Broker Configuration
MQTT_HOST = mqtt.hsl.fi
MQTT_PORT = 8883
MQTT_USERNAME = digitransit
