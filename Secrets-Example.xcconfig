// Rename this file to "Secrets.xcconfig" and fill in your values.
// Do not commit "Secrets.xcconfig" to source control.

// Digitransit API Key (Required)
// Get one at https://portal.digitransit.fi/
DIGITRANSIT_API_KEY = replace_me

// MQTT Configuration
// These values are used by the app for real-time updates.
// Defaults are provided in code if these are missing.
MQTT_HOST = mqtt.hsl.fi
MQTT_PORT = 8883
MQTT_USERNAME = digitransit
